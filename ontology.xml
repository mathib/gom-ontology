    <!-- http://creativecommons.org/ns#license -->


    <owl:ObjectProperty rdf:about="http://creativecommons.org/ns#license"/>
    


    <!-- https://w3id.org/gom#createdIn -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gom#createdIn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/gom#GeometryModellingApplication"/>
        <schema:domainIncludes rdf:resource="https://w3id.org/omg#Geometry"/>
        <schema:domainIncludes rdf:resource="https://w3id.org/omg#GeometryState"/>
        <rdfs:comment xml:lang="en">property to link to the geometry modelling application the geometry description was created in</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gom#"/>
        <rdfs:label xml:lang="en">created in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gom#fromCartesianCoordinateSystem -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gom#fromCartesianCoordinateSystem">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/gom#fromCoordinateSystem"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/gom#AffineCoordinateSystemTransformation"/>
        <rdfs:range rdf:resource="https://w3id.org/gom#CartesianCoordinateSystem"/>
        <rdfs:comment xml:lang="en">links to an instance of gom:CartesianCoordinateSystem where the transformation results in</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gom#"/>
        <rdfs:label xml:lang="en">from Cartesian Coordinate System</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gom#fromCoordinateSystem -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gom#fromCoordinateSystem">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/gom#CoordinateSystemTransformation"/>
        <rdfs:range rdf:resource="https://w3id.org/gom#CoordinateSystem"/>
        <rdfs:comment xml:lang="en">links to an instance of gom:CoordinateSystem where the transformation results in</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gom#"/>
        <rdfs:label xml:lang="en">from Coordinate System</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gom#hasCoordinateSystem -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gom#hasCoordinateSystem">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/gom#CoordinateSystem"/>
        <schema:domainIncludes>omg:Geometry</schema:domainIncludes>
        <schema:domainIncludes>omg:GeometryState</schema:domainIncludes>
        <rdfs:comment xml:lang="en">Geometry description is defined in exactly one Coordinate System. This property links a omg:Geometry of omg:GeometryState instance to a gom:CoordinateSystem instance.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gom#"/>
        <rdfs:label xml:lang="en">has coordinate system</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gom#hasLengthUnit -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gom#hasLengthUnit">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/gom#CartesianCoordinateSystem"/>
        <rdfs:range rdf:resource="https://w3id.org/gom#LengthUnit"/>
        <rdfs:comment xml:lang="en">Each Cartesian Coordinate System has exactly one length unit. If such a Coordinate System does not link to a length unit, it is assumed to be in metre.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gom#"/>
        <rdfs:label xml:lang="en">has length unit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gom#hasSimplifiedGeometry -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gom#hasSimplifiedGeometry">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/gom#RepresentedAccuracyAnalysis"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/omg#Geometry"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/omg#GeometryState"/>
        <rdfs:comment xml:lang="en">The simplified geometry of a gom:RepresentedAccuracyAnalysis that is compared with a survey geometry.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gom#"/>
        <rdfs:label xml:lang="en">has simplified geometry</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gom#hasSurveyGeometry -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gom#hasSurveyGeometry">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/gom#RepresentedAccuracyAnalysis"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/omg#Geometry"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/omg#GeometryState"/>
        <rdfs:comment xml:lang="en">The survey geometry where a gom:RepresentedAccuracyAnalysis starts from.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gom#"/>
        <rdfs:label xml:lang="en">has survey geometry</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gom#toCartesianCoordinateSystem -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gom#toCartesianCoordinateSystem">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/gom#toCoordinateSystem"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/gom#AffineCoordinateSystemTransformation"/>
        <rdfs:range rdf:resource="https://w3id.org/gom#CartesianCoordinateSystem"/>
        <rdfs:comment xml:lang="en">links to an instance of gom:CartesianCoordinateSystem where the transformation is applied to</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gom#"/>
        <rdfs:label xml:lang="en">to Cartesian Coordinate System</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gom#toCoordinateSystem -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gom#toCoordinateSystem">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/gom#CoordinateSystemTransformation"/>
        <rdfs:range rdf:resource="https://w3id.org/gom#CoordinateSystem"/>
        <rdfs:comment xml:lang="en">links an instance of gom:CoordinateSystemTransformation to an instance of gom:CoordinateSystem where the transformation is applied to</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gom#"/>
        <rdfs:label xml:lang="en">to Coordinate System</rdfs:label>
    </owl:ObjectProperty>
    


