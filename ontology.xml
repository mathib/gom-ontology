    <!-- https://w3id.org/gom#columnMajorArray -->


    <rdfs:Datatype rdf:about="https://w3id.org/gom#columnMajorArray">
        <rdfs:comment xml:lang="en">a JSON column major array representing a square matrix. An example in Turtle of a literal representing a 2 X 2 matrix M: &quot;[m11,m21,m12,m22]&quot;^^gom:columnMajorArray</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gom#"/>
        <rdfs:label xml:lang="en">a column major array</rdfs:label>
    </rdfs:Datatype>
    


    <!-- https://w3id.org/gom#rowMajorArray -->


    <rdfs:Datatype rdf:about="https://w3id.org/gom#rowMajorArray">
        <rdfs:comment xml:lang="en">a JSON row major array representing a square matrix. An example in Turtle of a literal representing a 2 X 2 matrix M: &quot;[m11,m12,m21,m22]&quot;^^gom:rowMajorArray</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gom#"/>
